{
  "name": "@e280/kimura",
  "version": "3.0.4",
  "description": "A tool for interactively editing the transformation matrices of pixi objects.",
  "type": "module",
  "main": "x/transformer.js",
  "scripts": {
    "build": "run-s _clean _tsc _ln _scute",
    "start": "octo 'scute -vw' 'tsc -w' 'node --watch x/tests.test.js' 'http-server x'",
    "_clean": "rm -rf x && mkdir x",
    "_tsc": "tsc",
    "_ln": "run-s _ln-s _ln-assets",
    "_ln-s": "ln -s \"$(realpath s)\" x/s",
    "_ln-assets": "ln -s \"$(realpath assets)\" x/assets",
    "_scute": "scute -v"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/SukantPal/pixi-essentials.git"
  },
  "keywords": [
    "pixijs",
    "transfomer",
    "konva-transformer"
  ],
  "author": "Przemysław Gałęzki",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/e280/kimura/issues"
  },
  "homepage": "https://github.com/e280/kimura#readme",
  "files": [
    "x",
    "s"
  ],
  "dependencies": {
    "@e280/scute": "^0.1.1",
    "http-server": "^14.1.1",
    "npm-run-all": "^4.1.5",
    "pixi.js": "^8.14.0"
  },
  "devDependencies": {
    "tslib": "~2.0.1",
    "typescript": "~5.3.3"
  }
}
